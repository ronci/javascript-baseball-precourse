{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "plugins": ["prettier"],
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "operator-linebreak": ["error", "before"],
    "prettier/prettier": "error",
    "no-unused-vars": "warn"
  }
}
